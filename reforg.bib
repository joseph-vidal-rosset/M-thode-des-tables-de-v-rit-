@book{leibniz_recherches_1998,
	address = {Paris},
	title = {Recherches générales sur l'analyse des notions et des vérités},
	isbn = {9782130482826},
	abstract = {Les " Recherches générales sur l'analyse des notions et des vérités ", publiées ici pour la première fois en français, témoignent de l'état des recherches logiques de Leibniz en 1686, au moment de la première synthèse de sa philosophie, le " Discours de métaphysique ". Elles seules font comprendre comment certains thèmes de sa métaphysique, par exemple la critique du nécessitarisme ou la détermination conceptuelle de l'individuation, naissent d'une réflexion sur le principe du " calcul de la raison " : l'inhérence du prédicat dans le sujet des propositions vraies.Autour de ce texte sont rassemblés 38 opuscules restés eux aussi inédits en français, tous rédigés entre 1676 et 1705. Certains constituent des essais techniques, d'autres des présentations populaires de la logique. On y trouvera encore les principaux fragments relatifs à la " science générale " (sur le versant formel du projet d'encyclopédie), des développements plus denses sur la contingence des choses et la liberté des hommes, de petits traités scolastiques sur les abstraits et le statut des accidents, enfin des " résumés de métaphysique " (comme les " 24 propositions " que Heidegger a privilégiées).Ces traductions ont été effectuées à partir des dernières avancées de l'édition. En particulier, le travail de la Leibniz-Forschungstelle de Münster (Vorausediction) a été pris en compte pour corriger les éditions plus anciennes ou même, dans certains cas, pour mettre à la disposition du public de langue française des pages non encore complètement accessibles en latin.Cet ensemble restitue les principaux moments d'une philosophie de la logique que Leibniz n'a jamais voulu (ou su) rassembler dans une Somme. Contre la certitude cartésienne, il défend une doctrine cohérentiste et générative de la vérité. Contre l'inflation scolastique et ces néologismes, il prône une version originale du principe nominaliste d'économie des entités. Enfin, il entreprend l'élucidation rationnelle des termes qu'on disait " transcendantaux " (comme ens, non-ens, possibile) donnant à l'ontologie traditionnelle une précision et un visage résolument nouveaux.},
	language = {Français},
	publisher = {Presses Universitaires de France - PUF},
	author = {Leibniz, Gottfried Wilhem},
	month = mar,
	year = {1998}
}

@book{frege_ecrits_1994,
